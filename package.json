    "execa": "^6.1.0",
    "tree-kill": "^1.2.2"
  },
  "peerDependencies": {
    "vite": "^3.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "bumpp": "^8.2.1",
