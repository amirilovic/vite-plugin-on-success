{
  "name": "vite-plugin-on-success",
  "version": "1.0.0",
  "description": "Vite plugin to run process build success.",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "types": "./src/index.ts",
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "vite build",
    "test": "vitest",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amirilovic/vite-plugin-on-success.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amirilovic/vite-plugin-on-success/issues"
  },
  "homepage": "https://github.com/amirilovic/vite-plugin-on-success#readme",
  "dependencies": {
    "debug": "^4.3.4",
    "execa": "^8.0.1",
    "tree-kill": "^1.2.2"
  },
  "peerDependencies": {
    "vite": "^3 || ^4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vitest": "^0.34.6"
  }
}
